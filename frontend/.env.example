# API Configuration
REACT_APP_API_URL=http://localhost:8080/api/v1
REACT_APP_API_TIMEOUT=30000

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com

# App Configuration
REACT_APP_APP_NAME=Rice Monitor
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# Debug and Development
REACT_APP_DEBUG_MODE=true
REACT_APP_ENABLE_CONSOLE_LOGS=true
GENERATE_SOURCEMAP=true

# Performance and Optimization
REACT_APP_ENABLE_SERVICE_WORKER=false
REACT_APP_CACHE_DURATION=3600000

# External Services (optional)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_HOTJAR_ID=

# Deployment Configuration
REACT_APP_BUILD_DATE=
REACT_APP_COMMIT_HASH=

# Security
REACT_APP_CSP_NONCE=